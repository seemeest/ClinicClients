<Page x:Class="ClinicClients.Vive.PatientVive"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ClinicClients.Vive"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PatientVive">

    <Grid>
        <StackPanel>
            <Button Content="Сохранить" Width="120" Command="{Binding SendDataCommand}" />
            <DataGrid AutoGenerateColumns="True" Name="dataGrid"   ItemsSource="{Binding DataList}"/>
        </StackPanel>
    </Grid>
</Page>
